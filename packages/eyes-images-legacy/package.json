{
  "name": "eyes.images",
  "version": "3.8.7",
  "description": "Applitools Eyes SDK for working directly with images",
  "keywords": [
    "eyes.images",
    "applitools",
    "eyes",
    "test automation",
    "visual regression",
    "automation",
    "testing",
    "tests"
  ],
  "homepage": "https://applitools.com",
  "author": {
    "name": "Applitools Team",
    "email": "team@applitools.com",
    "url": "https://applitools.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/applitools/eyes.sdk.javascript1.git"
  },
  "bugs": {
    "url": "https://github.com/applitools/eyes.sdk.javascript1/issues"
  },
  "directories": {
    "example": "./example",
    "lib": "./src",
    "test": "./test"
  },
  "files": [
    "README.md",
    "LICENSE",
    "index.js",
    "src/",
    "dist/eyes-browser.min.js"
  ],
  "main": "./index.js",
  "types": "./typings/index.d.ts",
  "dependencies": {
    "eyes.sdk": "^4.0.5",
    "eyes.utils": "^3.8.1"
  },
  "devDependencies": {
    "@applitools/sdk-release-kit": "0.1.0",
    "@types/node": "*",
    "browserify": "^16.5.0",
    "browserify-fs": "^1.0.0",
    "browserify-zlib": "^0.2.0",
    "mocha": "^6.2.2",
    "node-fetch": "^2.6.0",
    "typescript": "^3.6.4",
    "uglify-js": "^3.6.5"
  },
  "scripts": {
    "vv": "bongo --verify-versions",
    "vc": "bongo --verify-changelog",
    "uc": "bongo --update-changelog",
    "verify-commits": "bongo --verify-commits",
    "test": "npm run test:e2e",
    "test:e2e": "node test/simplerun.js",
    "build": "npm run browserify && npm run uglify",
    "browserify": "browserify eyes-browser-loader.js > dist/eyes-browser.js",
    "uglify": "uglifyjs dist/eyes-browser.js > dist/eyes-browser.min.js",
    "typings": "tsc --declaration",
    "preversion": "yarn vv && yarn vc && yarn verify-commits",
    "version": "yarn uc && git add CHANGELOG.md",
    "postversion": "git push --follow-tags"
  },
  "license": "SEE LICENSE IN LICENSE",
  "engines": {
    "node": ">=8.9.0"
  }
}
