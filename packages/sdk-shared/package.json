    "chai-as-promised": "7.1.1",
    "chalk": "3.0.0",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "express": "4.17.1",
    "filenamify": "4.2.0",
    "handlebars": "4.7.6",
    "morgan": "1.10.0",
    "ncp": "2.0.0",
    "node-fetch": "2.6.1",
    "prettier": "1.18.2",
    "yargs": "15.3.1"
  },
  "devDependencies": {
    "@applitools/sdk-release-kit": "0.13.0",
    "eslint": "^6.6.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-mocha-no-only": "^1.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.7",
    "js-yaml": "^3.13.1",
    "mocha": "^8.1.3",
    "open": "^7.0.3",
    "semver": "^7.3.2"
