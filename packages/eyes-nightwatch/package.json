{
  "name": "@applitools/eyes-nightwatch",
  "version": "0.1.0",
  "description": "Applitools Eyes SDK for Nightwatch.js",
  "keywords": [
    "eyes-nightwatch",
    "eyes-rendering",
    "eyes-visual-grid",
    "applitools",
    "eyes",
    "test automation",
    "visual regression",
    "automation",
    "testing",
    "tests",
    "nightwatch",
    "nightwatchjs"
  ],
  "homepage": "https://applitools.com",
  "author": {
    "name": "Applitools Team",
    "email": "team@applitools.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/applitools/eyes.sdk.javascript1.git"
  },
  "bugs": {
    "url": "https://github.com/applitools/eyes.sdk.javascript1/issues"
  },
  "directories": {
    "example": "./example",
    "lib": "./lib",
    "test": "./test"
  },
  "main": "./index.js",
  "dependencies": {
    "@applitools/eyes-sdk-core": "12.5.4",
    "@applitools/visual-grid-client": "15.0.16"
  },
  "devDependencies": {
    "@applitools/sdk-coverage-tests": "^2.0.0",
    "@applitools/sdk-fake-eyes-server": "^2.0.0",
    "@applitools/sdk-release-kit": "^0.4.1",
    "@applitools/sdk-shared": "^0.4.0",
    "assert-rejects": "^1.0.0",
    "axios": "^0.19.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chromedriver": "^86.0.0",
    "cors": "^2.8.5",
    "eslint": "^7.9.0",
    "eslint-plugin-mocha-no-only": "^1.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "geckodriver": "^1.20.0",
    "mocha": "^8.0.1",
    "ncp": "^2.0.0",
    "nightwatch": "1.3.4",
    "nock": "^12.0.3",
    "node-fetch": "^2.6.0",
    "prettier": "1.19.0",
    "spec-xunit-file": "0.0.1-3",
    "yargs": "^15.3.1"
  },
  "peerDependencies": {
    "nightwatch": ">=1.3.4"
  },
  "scripts": {
    "nightwatch": "nightwatch --config test/nightwatch.conf.js",
    "lint": "eslint . --ext .js",
    "render": "node ../sdk-shared/scripts/render.js",
    "test": "yarn test:it",
    "test:it": "yarn test:it:headless:jwp && yarn test:it:headless:w3c && yarn test:it:headed && yarn test:it:mobile",
    "test:it:headless:jwp": "yarn nightwatch test/it/spec-driver-headless-desktop.spec.js",
    "test:it:headless:w3c": "yarn nightwatch --env firefox test/it/spec-driver-headless-desktop.spec.js",
    "test:it:headed": "yarn nightwatch --env firefox test/it/spec-driver-onscreen-desktop.spec.js",
    "test:it:mobile": "yarn nightwatch test/it/spec-driver-mobile.spec.js",
    "coverage:generate": "coverage-tests generate test/coverage/index.js",
    "coverage:sandbox": "yarn coverage:generate && yarn test:coverage && coverage-tests process-report",
    "coverage:prod": "yarn coverage:generate && yarn test:coverage && coverage-tests process-report --send-report prod",
    "docker:setup": "node ../sdk-shared/src/generate-docker-compose-config.js && docker-compose up -d",
    "docker:teardown": "docker-compose down",
    "test:coverage": "APPLITOOLS_BATCH_NAME='JS Coverage Tests: eyes-nightwatch' APPLITOOLS_BATCH_ID=$(uuidgen) XUNIT_FILE=coverage-test-report.xml yarn nightwatch test/coverage/generic/*.spec.js",
    "types:generate": "rm -rf ./typings && tsc --declaration --declarationDir ./typings --allowJs --emitDeclarationOnly ./index.js",
    "preversion": "bongo preversion && yarn test && yarn coverage:prod",
    "version": "bongo version",
    "postversion": "bongo postversion",
    "deps": "bongo deps"
  },
  "license": "SEE LICENSE IN LICENSE",
  "engines": {
    "node": ">=8.9.0"
  }
}
