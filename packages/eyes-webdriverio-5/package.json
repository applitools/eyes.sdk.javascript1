{
  "name": "@applitools/eyes-webdriverio",
  "version": "5.25.1",
  "description": "Applitools Eyes SDK for WebdriverIO",
  "keywords": [
    "eyes-webdriverio",
    "applitools",
    "eyes",
    "test automation",
    "visual regression",
    "automation",
    "testing",
    "tests",
    "webdriverio"
  ],
  "homepage": "https://applitools.com",
  "author": {
    "name": "Applitools Team",
    "email": "team@applitools.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:applitools/eyes.sdk.javascript1.git"
  },
  "bugs": {
    "url": "https://github.com/applitools/eyes.sdk.javascript1/issues"
  },
  "directories": {
    "example": "./example",
    "lib": "./src",
    "test": "./test"
  },
  "main": "./index.js",
  "dependencies": {
    "@applitools/eyes-sdk-core": "12.4.2",
    "@applitools/visual-grid-client": "15.0.10"
  },
  "peerDependencies": {
    "webdriverio": "5.0.0 - 6.x.x"
  },
  "devDependencies": {
    "@applitools/sdk-coverage-tests": "link:../sdk-coverage-tests",
    "@applitools/sdk-fake-eyes-server": "link:../sdk-fake-eyes-server",
    "@applitools/sdk-release-kit": "link:../sdk-release-kit",
    "@applitools/sdk-shared": "link:../sdk-shared",
    "@wdio/cli": "5.18.6",
    "@wdio/dot-reporter": "5.18.6",
    "@wdio/local-runner": "5.18.6",
    "@wdio/mocha-framework": "5.18.6",
    "@wdio/sync": "5.18.6",
    "axios": "^0.19.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chromedriver": "^85.0.0",
    "eslint": "^7.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-mocha-no-only": "^1.1.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-testcafe": "^0.2.1",
    "geckodriver": "^1.19.1",
    "mocha": "^8.0.1",
    "module-alias": "^2.2.2",
    "prettier": "1.19.0",
    "shared-examples-for": "^0.1.3",
    "spec-xunit-file": "0.0.1-3",
    "typescript": "^3.9.6",
    "webdriverio": "^6.2.0",
    "webdriverio-5": "npm:webdriverio@^5.18.0",
    "webdriverio-6": "npm:webdriverio@^6.2.0"
  },
  "scripts": {
    "lint": "eslint '**/*.js'",
    "render": "node ../sdk-shared/scripts/render.js",
    "test": "yarn test:unit && yarn test:it && yarn test:e2e && yarn test:service",
    "test:e2e": "mocha --no-timeouts \"test/+(TestClassicApi*.js|TestFluentApi*.js|TestVisualGrid*.js)\"",
    "test:it": "mocha -r ./test/util/version-alias.js --no-timeouts \"test/it/*.js\"",
    "test:service": "wdio \"test/service/wdio.conf.js\" && wdio \"test/service/no-config/wdio.no-config.conf.js\" && wdio \"test/service/vg/wdio.vg.conf.js\"",
    "test:unit": "mocha \"test/unit/**/*.js\"",
    "test:coverage": "APPLITOOLS_BATCH_NAME='JS Coverage Tests: eyes-webdriverio-5' APPLITOOLS_BATCH_ID=$(uuidgen) XUNIT_FILE=coverage-test-report.xml mocha --config .mocharc.cvg.js",
    "coverage:generate": "coverage-tests generate ../sdk-shared/coverage-tests/generic/index.js --name 'eyes.webdriverio.javascript5'",
    "coverage:report": "coverage-tests report ../sdk-shared/coverage-tests/generic/index.js --name 'eyes.webdriverio.javascript5'",
    "coverage:sandbox": "yarn coverage:generate && yarn test:coverage && yarn coverage:report --sandbox",
    "coverage:prod": "yarn coverage:generate && yarn test:coverage && yarn coverage:report",
    "docker:setup": "node ../sdk-shared/src/generate-docker-compose-config.js && docker-compose up -d",
    "docker:teardown": "docker-compose down",
    "preversion": "bongo preversion && yarn test && yarn coverage:prod",
    "version": "bongo version",
    "postversion": "bongo postversion",
    "deps": "bongo deps"
  },
  "license": "SEE LICENSE IN LICENSE",
  "engines": {
    "node": ">=8.9.0"
  }
}
