    "test:sanity": "mocha --no-timeouts 'test/!(browser||e2e)/**/*.test.js'",
    "test:unit": "mocha --no-timeouts \"test/unit/**/*.test.js\"",
    "test:it": "mocha --no-timeouts \"test/it/**/*.test.js\"",
    "test:e2e": "mocha --no-timeouts \"test/e2e/**/*.test.js\"",
