{
  "name": "@applitools/eyes-images-browser",
  "private": true,
  "version": "4.6.62",
  "description": "Applitools Eyes Images SDK for browser",
  "keywords": [
    "eyes-images",
    "applitools",
    "eyes",
    "test automation",
    "visual regression",
    "automation",
    "testing",
    "tests",
    "browser"
  ],
  "homepage": "https://applitools.com",
  "author": {
    "name": "Applitools Team",
    "email": "team@applitools.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/applitools/eyes.sdk.javascript1.git"
  },
  "bugs": {
    "url": "https://github.com/applitools/eyes.sdk.javascript1/issues"
  },
  "directories": {
    "dist": "./dist"
  },
  "main": "./index.js",
  "dependencies": {
    "@applitools/eyes-images": "^4.12.33"
  },
  "devDependencies": {
    "@applitools/sdk-release-kit": "0.1.0",
    "browserify": "^16.2.3",
    "uglify-es": "^3.3.9"
  },
  "scripts": {
    "vv": "bongo --verify-versions",
    "vc": "bongo --verify-changelog",
    "uc": "bongo --update-changelog",
    "verify-commits": "bongo --verify-commits",
    "lint": "eslint . --ext .js --no-eslintrc --config ../../.eslintrc",
    "browserify": "browserify index.js > dist/eyes-images.browser.js",
    "uglify": "uglifyjs dist/eyes-images.browser.js > dist/eyes-images.browser.min.js",
    "build": "npm run browserify && npm run uglify",
    "preversion": "yarn vv && yarn vc && yarn verify-commits",
    "version": "yarn uc && git add CHANGELOG.md",
    "postversion": "git push --follow-tags"
  },
  "license": "SEE LICENSE IN LICENSE",
  "engines": {
    "node": ">= 8.9.0"
  }
}
