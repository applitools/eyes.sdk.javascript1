{
  "name": "@applitools/eyes-testcafe",
  "private": true,
  "version": "2.0.0-beta.20",
  "description": "Applitools Eyes SDK for Testcafe",
  "homepage": "https://applitools.com",
  "author": {
    "name": "Applitools Team",
    "email": "team@applitools.com"
  },
  "files": [
    "dist",
    "lib",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/applitools/eyes.sdk.javascript1.git"
  },
  "bugs": {
    "url": "https://github.com/applitools/eyes.sdk.javascript1/issues"
  },
  "directories": {
    "lib": "./lib",
    "dist": "./dist"
  },
  "main": "./index.js",
  "dependencies": {
    "@applitools/dom-utils": "4.7.16",
    "@applitools/eyes-sdk-core": "10.1.2",
    "@applitools/functional-commons": "1.5.4",
    "@applitools/visual-grid-client": "14.0.1",
    "rimraf": "^3.0.0"
  },
  "peerDependencies": {
    "testcafe": "^1.5.0"
  },
  "license": "SEE LICENSE IN LICENSE",
  "engines": {
    "node": ">=10.13.0"
  },
  "keywords": [
    "eyes-testcafe",
    "eyes-rendering",
    "eyes-visual-grid",
    "applitools",
    "eyes",
    "test automation",
    "visual regression",
    "automation",
    "testing",
    "tests",
    "testcafe"
  ],
  "scripts": {
    "lint": "eslint . --ext .js --config ../../.eslintrc",
    "build": "rollup -c ./build/rollup.config.js",
    "render": "node scripts/render.js",
    "test": "yarn test:unit && yarn test:it && yarn test:e2e",
    "test:unit": "mocha --no-timeouts \"test/unit/**/*.spec2.js\"",
    "test:it": "mocha --no-timeouts \"test/it/**/*.spec2.js\"",
    "test:e2e": "mocha --no-timeouts \"test/e2e/**/*.spec2.js\"",
    "test:perf": "mocha --no-timeouts \"test/perf/**/*.spec.js\"",
    "coverage:sandbox": "coverage-tests create-tests && yarn test:coverage && coverage-tests process-report",
    "coverage:prod": "coverage-tests create-tests && yarn test:coverage && coverage-tests process-report --send-report prod",
    "test:coverage": "env  APPLITOOLS_BATCH_NAME='JS Coverage Tests: eyes-testcafe' APPLITOOLS_BATCH_ID=$(uuidgen) testcafe chrome:headless test/coverage/generic --concurrency 15 --reporter spec,xunit:coverage-test-report.xml",
    "preversion": "bongo preversion && coverage:prod",
    "version": "bongo version",
    "postversion": "bongo postversion"
  },
  "devDependencies": {
    "@applitools/dom-capture": "^7.1.2",
    "@applitools/sdk-fake-eyes-server": "^2.0.0",
    "@applitools/sdk-release-kit": "0.1.0",
    "@applitools/sdk-coverage-tests": "1.0.0",
    "@babel/plugin-transform-async-to-generator": "^7.7.4",
    "@babel/plugin-transform-regenerator": "^7.7.5",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/plugin-transform-spread": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@joseph184/rollup-plugin-node-builtins": "^2.1.4",
    "assert-rejects": "^1.0.0",
    "axios": "^0.19.0",
    "chai": "^4.2.0",
    "chromedriver": "^80.0.0",
    "core-js": "^3.6.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "filenamify": "^4.1.0",
    "mocha": "^6.2.2",
    "rollup": "^1.27.9",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-polyfill": "^3.0.0",
    "testcafe": "^1.5.0",
    "testcafe-browser-provider-browserstack": "^1.10.1",
    "testcafe-browser-provider-saucelabs": "^1.8.0",
    "ua-parser-js": "^0.7.20",
    "url-polyfill": "^1.1.7",
    "whatwg-fetch": "^3.0.0"
  }
}