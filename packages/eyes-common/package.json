  "main": "./index.js",
  "browser": "./browser.js",
  "dependencies": {
    "cosmiconfig": "^6.0.0",
    "dateformat": "^3.0.3",
    "debug": "^4.1.1",
    "deepmerge": "^4.2.2",
    "png-async": "^0.9.4",
    "stack-trace": "^0.0.10"
  },
  "devDependencies": {
    "@applitools/sdk-release-kit": "0.1.0",
    "chai": "^4.2.0",
    "mocha": "^6.2.2"
  },
  "scripts": {
    "release-pre-check": "bongo --release-pre-check",
    "lint": "eslint . --ext .js --no-eslintrc --config ../../.eslintrc",
